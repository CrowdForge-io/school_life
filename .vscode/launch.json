  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Release",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--enable-software-rendering"
      ],
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "Flutter Profile",
      "type": "dart",
      "request": "launch",
      "args": [
        "--enable-software-rendering"
      ],
      "program": "lib/main.dart",
      "flutterMode": "profile",
    },
    {
      "name": "Flutter Debug",
      "request": "launch",
      "args": [
        "--enable-software-rendering"
      ],
      "program": "lib/main.dart",
      "flutterMode": "debug",
      "type": "dart"
    }
  ]
}